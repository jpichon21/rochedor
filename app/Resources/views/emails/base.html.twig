    body {
      margin: 0;
      padding: 0;
      -webkit-text-size-adjust: 100%;
      -ms-text-size-adjust: 100%;
    }

    table,
    td {
      border-collapse: collapse;
      mso-table-lspace: 0pt;
      mso-table-rspace: 0pt;
    }

    img {
      border: 0;
      height: auto;
      line-height: 100%;
      outline: none;
      text-decoration: none;
      -ms-interpolation-mode: bicubic;
    }

    p {
      display: block;
      margin: 13px 0;
    }

    @media (min-width: 1200px) {
      .detailCart-render .table-mobile {
        display: none;
      }
    }

    @media (max-width: 1199px) {
      .detailCart-render .table {
        display: none;
      }
    }

    .detailCart-render .table th.first,
    .detailCart-render .table td.first {
      max-width: 100px;
    }

    .detailCart-render .table th .info-total,
    .detailCart-render .table td .info-total {
      padding-top: 16px;
      font-style: italic;
      font-size: 13px;
    }

    .detailCart-render .table {
      width: 100%;
      margin-bottom: 20px;
      font-family: 'lato', lato, sans-serif;
      color: #ef7d1d;
    }

    .detailCart-render .table thead {
      font-size: 12px;
      font-weight: 500;
      letter-spacing: 1.4px;
      text-transform: uppercase;
    }

    .detailCart-render .table thead th {
      padding: 10px 0;
      font-size: 15px;
      font-weight: 400;
      text-align: center;
      text-transform: none;
      letter-spacing: 0;
      border-bottom: 1px #000000 solid;
      white-space: nowrap;
    }

    .detailCart-render .table thead th.first {
      padding-left: 0;
      text-align: left;
    }

    .detailCart-render .table thead th.last {
      padding-right: 0;
      text-align: right;
    }

    @media (max-width: 1023px) {
      .detailCart-render .table thead th.last {
        font-size: 0;
      }
    }

    .detailCart-render .table tbody tr {
      font-family: 'lato', lato, sans-serif;
    }

    .detailCart-render .table tbody tr:first-child td {
      padding-top: 15px;
    }

    .detailCart-render .table tbody tr:last-child td {
      padding-bottom: 15px;
    }

    .detailCart-render .table tbody tr.ttc td {
      padding-top: 40px;
      font-size: 16px;
      text-transform: uppercase;
      color: #ef7d1d;
    }

    .detailCart-render .table tbody tr td {
      padding: 10px 0 5px;
      font-size: 14px;
      color: #000000;
      text-align: center;
      white-space: nowrap;
    }

    .detailCart-render .table tbody tr td a {
      color: inherit;
      border-bottom: none !important;
    }

    .detailCart-render .table tbody tr td.action {
      font-size: 14px;
      font-family: lato, 'lato', sans-serif;
    }

    .detailCart-render .table tbody tr td.quantity .minus {
      margin-right: 10px;
    }

    .detailCart-render .table tbody tr td.quantity .plus {
      margin-left: 10px;
    }

    .detailCart-render .table tbody tr td.quantity i {
      color: #ef7d1d;
    }

    .detailCart-render .table tbody tr td.first {
      padding-left: 0;
      overflow: hidden;
      text-overflow: ellipsis;
      text-align: left;
    }

    .detailCart-render .table tbody tr td.last {
      padding-right: 0;
      text-align: right;
    }

    @media (max-width: 1023px) {
      .detailCart-render .table tbody tr td.last a {
        font-size: 0;
      }

      .detailCart-render .table tbody tr td.last a:before {
        content: '\274C';
        font-size: 10px;
      }
    }

    .detailCart-render .table tfoot td {
      padding: 15px 0;
      text-align: right;
      border-top: 1px #000000 solid;
      font-family: lato, 'lato', 'sans-serif';
      font-size: 14px;
    }

    .detailCart-render .table tfoot td span {
      margin-right: 10px;
    }
  </style>
