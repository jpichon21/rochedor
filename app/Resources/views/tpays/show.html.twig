{% extends 'base-admin.html.twig' %}

{% block body %}
    <h1>Tpay</h1>

    <table>
        <tbody>
            <tr>
                <th>Nompays</th>
                <td>{{ tpay.nompays }}</td>
            </tr>
            <tr>
                <th>Codpays</th>
                <td>{{ tpay.codpays }}</td>
            </tr>
            <tr>
                <th>Codpayspbx</th>
                <td>{{ tpay.codpayspbx }}</td>
            </tr>
            <tr>
                <th>Codpayspaypal</th>
                <td>{{ tpay.codpayspaypal }}</td>
            </tr>
            <tr>
                <th>Codpostaux</th>
                <td>{{ tpay.codpostaux }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('crud_tpays_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('crud_tpays_edit', { 'codpays': tpay.codpays }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
